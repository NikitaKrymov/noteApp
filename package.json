{
  "name": "note_app",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "engines": {
    "node": "8.1.1",
    "npm": "5.0.3"
  },
  "scripts": {
    "start": "node dist/index.js",
    "server": "nodemon --exec ts-node index.ts",
    "watch-node": "nodemon dist/index.js",
    "watch-ts": "tsc -w",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "deploy": "git add . && git commit -m Heroku && git push heroku master",
    "heroku-postbuild": "npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NikitaKrymov/notebooksApp.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NikitaKrymov/notebooksApp/issues"
  },
  "homepage": "https://github.com/NikitaKrymov/notebooksApp#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.2",
    "mongoose": "^5.10.9",
    "node": "^12.16.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.8",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.14.8",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.5",
    "ts-lint": "^4.5.1",
    "ts-node": "^3.3.0",
    "ts-node-dev": "^1.0.0-pre.65",
    "tsc": "^1.20150623.0",
    "typescript": "^4.0.5"
  }
}
